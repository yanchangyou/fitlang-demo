{
    "uni": "sequence",
    "child": [
        {
            "uni": "set",
            "key": "times",
            "value": 1
        },
        {
            "uni": "loop",
            "loopTimes": 10,
            "child": {
                "uni": "set",
                "key": "times",
                "value": "${times+1}"
            }
        },
        {
            "uni": "mix",
            "json": {
                "times": "${times}"
            }
        }
    ]
}